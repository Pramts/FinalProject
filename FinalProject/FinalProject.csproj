<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="icon\Exit" />
    <None Remove="icon\History" />
    <None Remove="icon\Home" />
    <None Remove="icon\Keranjang" />
    <None Remove="icon\Profile" />
    <None Remove="icon\Setting" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="icon\Exit">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="icon\History">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="icon\Home" />
    <Resource Include="icon\Keranjang" />
    <Resource Include="icon\Profile" />
    <Resource Include="icon\Setting">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

</Project>
